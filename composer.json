{
  "name": "tourze/json-rpc-check-ip-bundle",
  "description": "JsonRPC IP检查",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "symfony/config": "^6.4",
    "symfony/stopwatch": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/http-foundation": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "tourze/doctrine-helper": "0.0.*",
    "tourze/json-rpc-core": "^0.0.6",
    "psr/log": "^3|^2|^1"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\JsonRPCCheckIPBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\JsonRPCCheckIPBundle\\Tests\\": "src"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  }
}